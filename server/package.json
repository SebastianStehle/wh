{
  "name": "wh",
  "version": "1.0.0",
  "scripts": {
    "templ": "templ generate",
    "watch:templ": "templ generate -watch",
    "watch:go": "air -c .air",
    "watch:tailwind": "tailwindcss -i ./public/styles/styles.css -o ./public/css/styles.css --watch",
    "dev": "npm run templ && cross-env CGO_ENABLED=1 concurrently --restart-tries -1 --kill-others -n w: npm:watch:*"
  },
  "dependencies": {
    "color": "^4.2.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "daisyui": "^4.6.0",
    "tailwindcss": "^3.4.1"
  }
}
